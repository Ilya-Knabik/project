class animal:
    def __init__(self):
        self.name = 0
        self.age = 0
        self.health = 0
        self.hunger = 0

cow = animal()
cow.name = ""
cow.age = 6
cow.health = 100
cow.hunger = 0

pig = animal()
pig.name = ""
pig.age = 6
pig.health = 100
pig.hunger = 0

chicken = animal()
chicken.name = ""
chicken.age = 6
chicken.health = 100
chicken.hunger = 0
